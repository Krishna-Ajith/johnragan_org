<h1>Listing filter_demos</h1>
<p><%= message_for_view %>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for filter_demo in @filter_demos %>
  <tr>
    <td><%=h filter_demo.name %></td>
    <td><%= link_to 'Show', filter_demo %></td>
    <td><%= link_to 'Edit', edit_filter_demo_path(filter_demo) %></td>
    <td><%= link_to 'Destroy', filter_demo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New filter_demo', new_filter_demo_path %>
